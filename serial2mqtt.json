{
    "mqtt": {
        "connection": "tcp://limero.ddns.net:1883"
    },
    "serial": {
        "baudrate": 115200,
        "ports": [
            "/dev/rfcomm0"
        ],
        "protocol": "jsonArray"
    },
    "log": {
        "protocol": true,
        "debug": true,
        "useColors": true,
        "mqtt": false,
        "program": false,
        "level": "I",
        "file": "log.serial2mqtt.",
        "console": true
    }
}
